            <Label prefHeight="100.0" prefWidth="618.0" text="Gestion Statistiques" textFill="WHITE">
               <font>
                  <Font size="47.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <Button fx:id="btnStatReservation" layoutX="53.0" layoutY="166.0" mnemonicParsing="false" onAction="#menuStatClicked" prefHeight="78.0" prefWidth="143.0" text="Statistique reservations" />
      <Button fx:id="btnStatStation" layoutX="368.0" layoutY="166.0" mnemonicParsing="false" onAction="#menuStatClicked" prefHeight="78.0" prefWidth="143.0" text="Statistique stations" />
      <Button fx:id="btnStatUser" layoutX="211.0" layoutY="275.0" mnemonicParsing="false" onAction="#menuStatClicked" prefHeight="78.0" prefWidth="143.0" text="Statistique utilisateurs" />
